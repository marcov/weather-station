
<div class="heading">
  <div class="title">
    Meteo Fiobbio
  </div>
  <div>
    Stazione a <!--stationElevation-->&nbsp;, <!--stationLatitude-->- <!--stationLongitude-->
  </div>
  <!-- HEADER START -->
  <div style="font-size: 80%; padding-top: 0.5em;">
    Alba: <!--sunriseTime-->, tramonto: <!--sunsetTime-->. Fase lunare: <!--moonPhase-->
  </div>
  <!-- HEADER END -->
</div>

<div style="margin-top: 1em;">
  <ul class="measures-container">

    <li class="measure">
      <label>Vento</label>
      <div>
        <div class="other">
          Raffica<br><!--windGustSpeed--><div class="unit"><!--windUnit--></div>
        </div>
        <div class="curr">
          <!--windSpeed--><div class="unit"><!--windUnit--></div><br>
          <div style="font-size:50%">
            <!--windDirection-->&nbsp;<!--windBeaufortScale-->
          </div>
        </div>
        <div class="max">
          <!--hiWindSpeed--><div class="unit"><!--windUnit--></div><br>
          <!--hiWindSpeedTime-->
        </div>
      </div>
    </li>

    <li class="measure">
      <label>Temperatura</label>
      <div>
        <div class="min">
          <!--lowOutsideTemp--><label class="unit">C</label><br>
          <!--lowOutsideTempTime-->
        </div>
        <div class="curr">
          <!--outsideTemp-->12.3<label class="unit">C</label> <br>
          <div style="font-size: 50%;">
            Percepita &nbsp; <!--apparentTemp--><label class="unit">C</label>
          </div>
        </div>
        <div class="max">
        <!--hiOutsideTemp--><label class="unit">C</label><br>
          <!--hiOutsideTempTime-->
        </div>
      </div>
    </li>

    <li class="measure">
      <label>Umidit&agrave;</label>
      <div>
        <div class="min">
          <!--lowHumidity--><label class="unit">%</label><br>
          <!--lowHumTime-->
        </div>
        <div class="curr">
          <!--outsideHumidity--><label class="unit">%</label><br>
          <div style="font-size: 50%">&nbsp;</div>
        </div>
        <div class="max">
        <!--hiHumidity--> <label class="unit">%</label><br>
          <!--hiHumTime-->
        </div>
      </div>
    </li>

    <li class="measure">
      <label>Pressione</label>
      <div>
        <div class="min">
          <!--lowBarometer--><label class="unit"><!--barUnit--></label><br>
          <!--lowBarometerTime-->
        </div>
        <div class="curr">
          <!--barometer--><label class="unit"><!--barUnit--></label><br>
          <div style="font-size:50%">
            Tendenza&nbsp;<!--baromtrend-->
          </div>
        </div>
        <div class="max">
        <!--hiBarometer--><label class="unit"><!--barUnit--></label><br>
          <!--hiBarometerTime-->
        </div>
      </div>
    </li>

    <li class="measure">
      <label>Pioggia</label>
      <div>
        <div class="other">
          Tot<br><!--dailyRain--><label class="unit">mm</label>
        </div>
        <div class="curr">
          <!--rainRate--><label class="unit"><!--rateUnit--></label><br>
          <div style="font-size:50%">
          </div>
        </div>
        <div class="max">
        <!--stormRain--><label class="unit"><!--rateUnit--></label>
        </div>
      </div>
    </li>
  </ul>
</div>

<div>
  <ul class="webcams-container">
    <li>
      <a href="http://rete.centrometeolombardo.com/bergamo/fiobbio/public/webcam.jpg" data-featherlight="image">
